programa {
	funcao inicio() {
		para (inteiro i = 0; i <= 100; i++){
		    se (i%2 == 0){
		        escreva(i, " : é par\n")
		    }senao{
		        escreva(i, " : é impar\n")
		    }    
		}
		inteiro n = 0
		enquanto (n <= 100){
		    se (n%2 == 0){
		        escreva(n, " : é par\n")
		    }senao{
		        escreva(n, " : é impar\n")
		    } 
		    
		    n = n + 1
		}
	}
}
